pipeline {
    agent any
    tools {
        maven 'maven'
    }
    stages {
        stage('env') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('build') {
            steps {
                dir('demo/java') {
                    sh 'ls -al .'
                    sh 'mvn clean install -e'
                }
            }
        }
    }
}