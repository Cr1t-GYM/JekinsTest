pipeline {
    agent any
    tools {
        maven 'maven'
    }
    jdk = tool name: 'jdk8'
    env.JAVA_HOME = "${jdk}"
    stages {
        stage('env') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('build') {
            steps {
                sh 'mvn clean install -B --no-transfer-progress'
            }
        }
    }
}